  "dependencies": {
    "acorn": "^7.1.1",
    "animated": "^0.2.2",
    "axios": "^0.21.1",
    "chokidar-cli": "^2.1.0",
    "file-loader": "^3.0.1",
    "framer-motion": "^4.1.4",
    "google-map-react": "^1.1.6",
